{
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "jsxSingleQuote": false,
  "printWidth": 180,
  "overrides": [
    {
      "files": "*.jsx",
      "options": {
        "singleQuote": false
      }
    }
  ],
  "quoteProps": "as-needed",
  "rangeStart": 0,
  "semi": false,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "useTabs": false,
  "jsxBracketSameLine": true
}